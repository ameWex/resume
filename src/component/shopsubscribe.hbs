<section class="alert">
    {{#with subscribe}}
    <div class="card text-bg-dark">
        <h2 class="display-6 card-header">{{header}}</h2>
        <div class="card-body">
            <p>{{description}}</p>
            {{#each buttons}}
            <div>
                <a class="btn btn-primary mb-3" href="{{link}}">{{text}}</a>
            </div>
            {{/each}}
        </div>
    </div>
    {{/with}}
</section>