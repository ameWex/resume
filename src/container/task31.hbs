<main class="row col-12">
    {{#with navigation}}
    <div class="row">
        {{#each links}}
        <a href="{{href}}" class="btn btn-link col-2">{{text}}</a>
        {{/each}}
        {{#with button}}
        <div class="col-3">
            <a href="{{href}}" class="btn btn-primary">{{text}}</a>
        </div>
        {{/with}}
    </div>
    {{/with}}
    <hr>
    <br>
    {{#with header}}
    <div>
        <h1>{{title}}</h1>
        <p class="text-muted">{{description}}</p>
        {{#with button}}
        <a class="btn btn-primary" href="{{link}}">{{text}}</a>
        {{/with}}
    </div>
    {{/with}}
    <br>
    <hr>
    <br>
    {{#with city}}
    <h2>{{title}}</h2>
    <div class="row col-10">
        {{#each cards}}
        <div class="col-5 g-2">
            <div class="card">
                <img src="{{picture.src}}" alt="{{picture.alt}}">
                <div class="card-title">
                    <h3>{{title}}</h3>
                    <p class="card-body">{{description}}</p>
                    {{#with button}}
                    <a class="btn btn-primary col-3" href="{{href}}">{{text}}</a>
                    {{/with}}
                </div>
            </div>
        </div>
    </div>
    {{/each}}
    {{/with}}
    <br>
    {{#with winner}}
    <div>
        <h2>{{title}}</h2>
        {{#with table}}
        <table class="table">
            <thead>
                <tr>
                    {{#each head}}
                    <th>{{this}}</th>
                    {{/each}}
                </tr>
            </thead>
            <tbody>
                {{#each user}}
                <tr>
                    <td>{{place}}</td>
                    <td>{{name}}</td>
                    <td>{{surname}}</td>
                    <td>{{nickname}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        {{/with}}
    </div>
    {{/with}}
    <br>
    {{#with gallery}}
    <div>
        <h2>{{title}}</h2>
        <div class="row col-12">
            <div class="card">
                {{#each cards}}
                <div class="col-4 g-3">
                    <div class="card-header">{{name}}</div>
                    {{#each items}}
                    <div class="list-group list-group-flush">
                        <div class="list-group-item">
                            {{this}}
                        </div>
                    </div>
                    {{/each}}
                </div>
                {{/each}}
            </div>
        </div>
    </div>
    <div>
        <h2>{{title}}</h2>
        <div class="row col-12">
            <div class="card">
                {{#each cards}}
                <div class="col-4 g-3">
                    <div class="card-header">{{name}}</div>
                    {{#each items}}
                    <div class="list-group list-group-flush">
                        <div class="list-group-item">
                            {{this}}
                        </div>
                    </div>
                    {{/each}}
                </div>
                {{/each}}
            </div>
        </div>
    </div>

    <div>
        <h2>{{title}}</h2>
        <div class="row col-12">
            <div class="card">
                {{#each cards}}
                <div class="col-4 g-3">
                    <div class="card-header">{{name}}</div>
                    {{#each items}}
                    <ul class="list-group list-group-flush">
                        <li class="list-group-item">
                            {{this}}
                        </li>
                    </ul>
                    {{/each}}
                </div>
                {{/each}}
            </div>
        </div>
    </div>
    {{/with}}
    </div>
</main>