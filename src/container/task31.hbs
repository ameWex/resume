<main class="">
    {{#with navigation}}
    <div class="row">
        {{#each links}}
        <a href="{{href}}" class="btn btn-link col-2">{{text}}</a>
        {{/each}}
        {{#with button}}
        <div class="col-3">
            <a href="{{href}}" class="btn btn-primary">{{text}}</a>
        </div>
        {{/with}}
    </div>
    {{/with}}
    <hr>
    <br>
    {{#with header}}
    <div>
        <h1>{{title}}</h1>
        <p class="text-muted">{{description}}</p>
        {{#with button}}
        <a class="btn btn-primary" href="{{link}}">{{text}}</a>
        {{/with}}
    </div>
    {{/with}}
    <br>
    <hr>
    <br>
    {{#with city}}
    <div>
        <h2>{{title}}</h2>
    </div>
    <br>
    {{#each cards}}
    <div class="row">
        <div class="card col-5 g-3">
            <img src="{{src}}" alt="{{alt}}" class="card-img-top">
            <div class="card-title">{{title}}</div>
            <p class="card-body">{{description}}</p>
            {{#with button}}
            <a class="btn btn-primary col-3" href="{{href}}">{{text}}</a>
            {{/with}}
        </div>
    </div>
    {{/each}}
    {{/with}}
    <br>
    {{#with winner}}
    <div>
        <h2>{{title}}</h2>
        {{#with table}}
        <table class="table">
            <thead>
                <tr>
                    {{#each head}}
                    <th>{{this}}</th>
                    {{/each}}
                </tr>
            </thead>
            <tbody>
                {{#each user}}
                <tr>
                    <td>{{place}}</td>
                    <td>{{name}}</td>
                    <td>{{surname}}</td>
                    <td>{{nickname}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
        {{/with}}
    </div>
    {{/with}}
    <br>
    {{#with gallery}}
    <div class="row">
        <div class="col-12">
            <div class="">
                <div class="card col-4">
                    <h2>{{title}}</h2>
                    {{#each cards}}
                    <div class="">
                        <div class="card-header">
                            <div>{{name}}</div>
                        </div>
                        {{#each items}}
                        <ul class="list-group list-group-flush">
                            <li class="list-group-item">
                                {{this}}
                            </li>
                        </ul>
                        {{/each}}
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>
        <div class="">
            <div class="col-12">
                <div class="card col-4">
                    <h2>{{title}}</h2>
                    {{#each cards}}
                    <div class="">
                        <div class="card-header">
                            <div>{{name}}</div>
                        </div>
                        {{#each items}}
                        <ul class="list-group list-group-flush">
                            <li class="list-group-item">
                                {{this}}
                            </li>
                        </ul>
                        {{/each}}
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>

        <div class="">
            <div class="col-12">
                <div class="card col-4">
                    <h2>{{title}}</h2>
                    {{#each cards}}
                    <div class="">
                        <div class="card-header">
                            <div>{{name}}</div>
                        </div>
                        {{#each items}}
                        <ul class="list-group list-group-flush">
                            <li class="list-group-item">
                                {{this}}
                            </li>
                        </ul>
                        {{/each}}
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>
        {{/with}}
    </div>
</main>